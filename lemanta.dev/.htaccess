    RewriteRule  ^stocks/([^/]+(/[^/]+)*)/?$                   index.php?module=Stock&url=$1          [NC,L,QSA]



    # Оплата ВКонтакте

    RewriteRule  ^vk_fail/?$     index.php?module=Order&vk=fail&quickcontent=1     [NC,L,QSA]
    RewriteRule  ^vk_process/?$  index.php?module=Order&vk=process&quickcontent=1  [NC,L,QSA]
