# ----------------------------------------------------------------------------
#
# Анти много слешевый редирект (например http://site.com/path/////page-name)

    RewriteCond %{REQUEST_URI}   ^(.*?)[/\\\\][/\\\\]+(.*?)$
    RewriteRule ^.*$             %1/%2  [R=301,L,QSA]

# ----------------------------------------------------------------------------
#
# Убрать теневые страницы каталога

    RewriteCond %{REQUEST_URI}  ^/catalog/  [NC]
    RewriteRule ^.*$            /           [R=301,L,QSA]

# ----------------------------------------------------------------------------
#
# Убрать теневую страницу главной

    RewriteCond %{REQUEST_URI}  ^[/\\\\]sections[/\\\\]mainpage[/\\\\]?$  [NC]
    RewriteRule ^.*$            http://%{HTTP_HOST}/                      [R=301,L,QSA]

# ----------------------------------------------------------------------------
#
# Пагинация и брендовая часть URL через дефисы

    RewriteCond %{REQUEST_URI}        /page_[0-9]+$  [NC]
    RewriteRule ^(.*/page)_([0-9]+)$  $1-$2          [R=301,L,QSA]

    RewriteCond %{REQUEST_URI}                 ^(/[^/\\\\]+)+/filter_.+$  [NC]
    RewriteRule ^((/[^/\\\\]+)+/filter)_(.+)$  $1-$3                      [R=301,L,QSA]

# ----------------------------------------------------------------------------
#
# Убрать тень первой страницы пагинации

    RewriteCond %{REQUEST_URI}         /page[_\-][0-1]$        [NC]
    RewriteRule ^(.*)/page[_\-][0-1]$  http://%{HTTP_HOST}/$1  [R=301,L,QSA]
    
# ----------------------------------------------------------------------------
#
# Анти верхне регистровый редирект (например http://site.com/Path/Page-Name)
#
# ВАЖНО: действует при условии, что в конфигурации сервера или VirtualHost
#        прописана следующая директива:
#
#            RewriteMap  lc  int:tolower
#

    RewriteCond  %{REQUEST_URI}  [A-Z]
    RewriteCond  %{REQUEST_URI}  !\.(jpe?g|png|gif|ico|mp4|ttf|svg|woff2?|eot|js|css|pdf|doc|zip|mp3|wav)$  [NC]
    RewriteRule  ^(.*)$          /${lc:$1}  [R=301,L,QSA]

# ----------------------------------------------------------------------------
#
# Слеш-БезСлеш редирект

    RewriteCond  %{REQUEST_URI}  ^/.*/$
    RewriteCond  %{REQUEST_URI}  !^/admin/$
    RewriteRule  ^(.*?)/+$       http://%{HTTP_HOST}/$1  [R=301,L,QSA]



# ------------------ AIMatrix DDoS inspector (начало) ------------------------
#
# Работает только если на веб сервере запущен модуль SetEnvIf

<IfModule setenvif_module>

    # Для включение инспектора установите в следующей строке INSPECTOR_STATE=on
    # Для отключение установите INSPECTOR_STATE=off
    #
    SetEnvIf REMOTE_ADDR ^.*$ INSPECTOR_STATE=off



    # Инициируем в окружении переменную типа пользователя значением "незнакомец"
    #
    SetEnvIf INSPECTOR_STATE ^.*$ USER_NATURE=stranger



    # Проверяем IP подключившегося компьютера на принадлежность к клиентам сайта
    # (ниже сразу за опорной строкой перечисляются все строки пропуска по IP-адресу
    # одна за другой, в результате при обнаружении соответствия IP-адреса какой-то
    # из этих строк установится переменная окружения USER_NATURE в значение client).
    #
    # Важно! Не перемещайте эти строки, не передвигайте, они заполняются скриптом-инспектором!
    # Важно! Не удаляйте и не изменяйте опциональные timestamp-комментарии в конце
    # этих строк, на них опирается скрипт-инспектор!
    #
    # Формат пропускающих строк (в квадратных скобках указана опциональная часть):
    # SetEnvIf REMOTE_ADDR ^client_ip USER_NATURE=client [# timestamp # дата]
    #
    # DDoS inspector CLIENT IPs LIST (не удаляйте эту строку, она опорная для скрипта-инспектора)



    # Проверяем IP подключившегося компьютера на принадлежность к атакующим
    # (ниже сразу за опорной строкой перечисляются все строки блокирования по IP-адресу
    # одна за другой, в результате при обнаружении соответствия IP-адреса какой-то
    # из этих строк установится переменная окружения USER_NATURE в значение bad или же
    # в значение ignore для записей, созданных режимом пропуска лишь единственно обслуживаемых
    # хостов).
    #
    # Важно! Не перемещайте эти строки, не передвигайте, они заполняются скриптом-инспектором!
    # Важно! Не удаляйте и не изменяйте опциональные timestamp-комментарии в конце
    # этих строк, на них опирается скрипт-инспектор!
    #
    # Формат блокирующих строк (в квадратных скобках опциональная часть):
    # SetEnvIf REMOTE_ADDR ^bad_ip USER_NATURE=bad [# timestamp # дата # имя хоста]
    #
    # DDoS inspector BAD IPs LIST (не удаляйте эту строку, она опорная для скрипта-инспектора)



    # Проверяем IP подключившегося компьютера на принадлежность к поисковым роботам
    # (ниже сразу за опорной строкой перечисляются все строки пропуска роботов по IP-адресу
    # одна за другой, в результате при обнаружении соответствия IP-адреса какой-то
    # из этих строк установится переменная окружения USER_NATURE в значение spider).
    #
    # Важно! Не перемещайте эти строки, не передвигайте, они заполняются скриптом-инспектором!
    #
    # Формат строк пропуска поисковых роботов (в квадратных скобках опциональная часть):
    # SetEnvIf REMOTE_ADDR ^spider_ip USER_NATURE=spider [# имя хоста # дата]
    #
    # DDoS inspector SPIDER IPs LIST (не удаляйте эту строку, она опорная для скрипта-инспектора)



    # ----------------------------------------------------------------------------
    #
    # Данное условие IfModule специально закомментировано, чтобы заключенные в него
    # инструкции mod_rewrit-а работали не только на Apache-серверах
    #
    # <IfModule rewrite_module>

        # Если это атакующий компьютер, запрещаем доступ

        RewriteCond  %{env:INSPECTOR_STATE}  ^on$            [NC]
        RewriteCond  %{env:USER_NATURE}      ^(bad|ignore)$  [NC]
        RewriteRule  ^.*$                    -               [F,L]



        # Если это еще не проверенный посетитель (не клиент сайта и не поисковый робот),
        # отправляем на верификацию к скрипту-инспектору

        RewriteCond  %{env:INSPECTOR_STATE}  ^on$                         [NC]
        RewriteCond  %{env:USER_NATURE}      !^(client|spider)$           [NC]
        RewriteRule  ^.*$                    AIMatrix_DDoS_inspector.php  [NC,L,QSA]



        # Если это проверенный посетитель (клиент сайта или поисковый робот) обращается
        # к скрипту-инспектору, задаем редирект на корень папки

        RewriteCond  %{env:INSPECTOR_STATE}              ^on$                    [NC]
        RewriteCond  %{env:USER_NATURE}                  ^(client|spider)$       [NC]
        RewriteRule  ^(.*)AIMatrix_DDoS_inspector\.php$  http://%{HTTP_HOST}/$1  [R=301,NC,L,QSA]

    # </IfModule>
    #
    # ----------------------------------------------------------------------------

</IfModule>

# ------------------ AIMatrix DDoS inspector (конец) -------------------------



# Стандартной кодировкой принимаем UTF-8

AddDefaultCharset  UTF-8



# При отсутствии документа перенаправляем на такой url

ErrorDocument  404  /sections/404



# ----------------------------------------------------------------------------
#
# Данное условие IfModule специально закомментировано, чтобы заключенные в него
# инструкции mod_rewrit-а работали не только на Apache-серверах
#
# <IfModule rewrite_module>

    # Если хост имеет вид www.нечто и входной url не robots.txt и не папка admin,
    # тогда задаем 301-й редирект на страницу вида http://нечто/входной_url

    RewriteCond  %{HTTP_HOST}    ^www\.(.+)$             [NC]
    RewriteCond  %{REQUEST_URI}  !^/robots\.txt$         [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/admin(/.+)?/$  [NC]
	RewriteCond  %{REQUEST_URI}  !^(/.+)?/webstat(/.+)?/$  [NC]
    RewriteRule  ^(.*)$          http://%1/$1            [R=301,L,QSA]



    # Если входной url равен /путь/index.php без параметров, задаем 301-й редирект на страницу http://сайт/путь/
    # (следующие 3 команды закомментированы, так как на некоторых серверах срабатывают ложно)

    # RewriteCond  %{REQUEST_URI}   ^/([^/]+/)*index\.php$  [NC]
    # RewriteCond  %{QUERY_STRING}  !^.+$                   [NC]
    # RewriteRule  ^(.*)$           http://%{HTTP_HOST}/%1  [R=301,L,QSA]



    # Если входной url оканчивается слешем и содержит папку (catalog или products или ... или stocks),
    # и не содержит папку (admin или на конце папку page_ЧИСЛО, cart, compare, quickБУКВЫ),
    # и хост не равен localhost, тогда задаем 301-й редирект на страницу вида http://сайт/входной_url
    # без слеша на конце.

    RewriteCond  %{REQUEST_URI}  ^/.+/$
    RewriteCond  %{REQUEST_URI}  ^(/.+)?/(catalog|products|items|kits|brands|articles|news|sections|media|stocks)(/.+)?/$  [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/admin(/.+)?/$                                                                    [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/page[_\-][0-9]+/$                                                                [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/cart/$                                                                           [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/compare/$                                                                        [NC]
    RewriteCond  %{REQUEST_URI}  !^(/.+)?/quick[a-z]*/$                                                                    [NC]
    RewriteCond  %{HTTP_HOST}    !^localhost$                                                                              [NC]
    RewriteRule  ^(.*)/$         http://%{HTTP_HOST}/$1                                                                    [R=301,L,QSA]



    # Если обращаются к файлам редиректов, запрещаем доступ

    RewriteRule  ^redirects[^/]*?\.txt$  -  [F,L]



    # Если входной url указывает на физическую папку или файл сайта, прекратить преобразование
    # (когда входной url совпадает с преобразованным, обработка прерывается)

    RewriteCond  %{REQUEST_FILENAME}  -d
    RewriteCond  %{REQUEST_URI}       !^(.*/)?captcha/?$  [NC]
    RewriteRule  ^(.*)$               -                   [L]

    RewriteCond  %{REQUEST_FILENAME}  -f
    RewriteCond  %{REQUEST_URI}       !^.+\.class\.php(/.*)?$      [NC]
    RewriteCond  %{REQUEST_URI}       !^(.*/)?captcha/image\.php$  [NC]
    RewriteRule  ^(.*)$               -                            [L]



    # Далее задаем правила (по одному в строке) для преобразования человеко-понятных url страниц.
    # Формат записи: что ищем во входном url, каким сделать выходной url, вспомогательные флаги правила.
    # Значения флагов:
    #   NC  = не обращать внимание на регистр символов входного url,
    #   L   = ничего больше не выполнять до конца файла,
    #   QSA = прикрепить к выходному url входные параметры запроса
    #
    # ----------------------------------------------------------------------------
    #
    # Защитный код

    RewriteRule  ^captcha/?$           index.php?module=Captcha  [NC,L,QSA]
    RewriteRule  ^captcha\.(jpg|php)$  index.php?module=Captcha  [NC,L,QSA]
    RewriteRule  ^captcha/image\.php$  index.php?module=Captcha  [NC,L,QSA]



    # Конфигуратор и быстрый заказ

    RewriteRule  ^configurator/?$  index.php?module=Configurator              [NC,L,QSA]
    RewriteRule  ^quickorder/?$    index.php?module=Configurator&cataloged=1  [NC,L,QSA]



    # Импорт

    RewriteRule  ^import/?$                        index.php?module=Import                       [NC,L,QSA]
    RewriteRule  ^import/([^/]+(/[^/]+)*)/info/?$  index.php?module=Import&url=$1&import_info=1  [NC,L,QSA]
    RewriteRule  ^import/([^/]+(/[^/]+)*)/?$       index.php?module=Import&url=$1                [NC,L,QSA]



    # Медиа файлы

    RewriteRule  ^media/?$                                    index.php?module=Files                 [NC,L,QSA]
    RewriteRule  ^media/page[_\-]([^/]+)/?$                   index.php?module=Files&page=$1         [NC,L,QSA]
    RewriteRule  ^media/([^/]+(/[^/]+)*)/page[_\-]([^/]+)/?$  index.php?module=Files&url=$1&page=$3  [NC,L,QSA]
    RewriteRule  ^media/([^/]+(/[^/]+)*)/?$                   index.php?module=Files&url=$1          [NC,L,QSA]



    # Страны, области, города

