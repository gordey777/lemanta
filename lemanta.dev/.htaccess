#
# Настройки кеширования

<IfModule mod_expires.c>
    ExpiresActive On

    # устанавливаем значения кэширования по умолчанию
    ExpiresDefault                                "access plus 12 hour"

    # кеширование манифестов
    ExpiresByType  text/cache-manifest            "access plus 0 seconds"

    # кеширование  html страниц
    ExpiresByType  text/html                      "access plus 1 hour"
    ExpiresByType  application/xhtml+xml          "access plus 1 hour"
    ExpiresByType  application/xml-dtd            "access plus 1 hour"

    # кеширование дизайна: стили, скрипты
    ExpiresByType  text/css                       "access plus 1 week"
    ExpiresByType  text/javascript                "access plus 1 week"
    ExpiresByType      application/javascript     "access plus 1 week"
    ExpiresByType      application/x-javascript   "access plus 1 week"
    ExpiresByType  text/x-jquery-tmpl             "access plus 1 week"

    # кеширование дизайна: веб-шрифты
    ExpiresByType  image/svg+xml                  "access plus 1 month"
    ExpiresByType  application/x-font-ttf         "access plus 1 month"
    ExpiresByType  application/x-font-woff        "access plus 1 month"
    ExpiresByType      application/font-woff      "access plus 1 month"
    ExpiresByType      application/x-woff         "access plus 1 month"
    ExpiresByType      application/octet-stream   "access plus 1 day"
    ExpiresByType  application/vnd.ms-fontobject  "access plus 1 month"

    # кеширование динамических данные
    ExpiresByType  text/xml                       "access plus 0 seconds"
    ExpiresByType      application/xml            "access plus 0 seconds"
    ExpiresByType  application/json               "access plus 0 seconds"

    # кеширование rss потока
    ExpiresByType  application/atom+xml           "access plus 1 hour"
    ExpiresByType  application/rss+xml            "access plus 1 hour"

    # кеширование ярлыковой иконки
    ExpiresByType  image/x-icon                   "access plus 1 week"
    ExpiresByType      image/ico                  "access plus 1 week"
    ExpiresByType      image/vnd.microsoft.icon   "access plus 1 week"

    # кеширование медиа файлов: картинки
    ExpiresByType  image/bmp                      "access plus 1 week"
    ExpiresByType  image/gif                      "access plus 1 week"
    ExpiresByType  image/jpg                      "access plus 1 week"
    ExpiresByType      image/jpeg                 "access plus 1 week"
    ExpiresByType      image/pjpeg                "access plus 1 week"
    ExpiresByType  image/png                      "access plus 1 week"
    ExpiresByType  image/svg+xml                  "access plus 1 week"
    ExpiresByType  image/tiff                     "access plus 1 week"

    # кеширование медиа файлов: аудио
    ExpiresByType  audio/amr                      "access plus 1 week"
    ExpiresByType  audio/basic                    "access plus 1 week"
    ExpiresByType  audio/midi                     "access plus 1 week"
    ExpiresByType  audio/mp4                      "access plus 1 week"
    ExpiresByType  audio/mpeg                     "access plus 1 week"
    ExpiresByType  audio/ogg                      "access plus 1 week"
    ExpiresByType      application/ogg            "access plus 1 week"
    ExpiresByType  audio/x-aiff                   "access plus 1 week"
    ExpiresByType  audio/x-mpegurl                "access plus 1 week"
    ExpiresByType  audio/x-ms-wma                 "access plus 1 week"
    ExpiresByType  audio/x-wav                    "access plus 1 week"
    ExpiresByType      audio/vnd.wave             "access plus 1 week"

    # кеширование медиа файлов: видео
    ExpiresByType  application/x-dvi              "access plus 1 week"
    ExpiresByType  video/jpeg                     "access plus 1 week"
    ExpiresByType  video/mpv                      "access plus 1 week"
    ExpiresByType  video/mpeg                     "access plus 1 week"
    ExpiresByType  video/quicktime                "access plus 1 week"
    ExpiresByType  video/vnd.mpegurl              "access plus 1 week"
    ExpiresByType  video/x-msvideo                "access plus 1 week"
    ExpiresByType  application/x-shockwave-flash  "access plus 1 week"

    # кеширование документов
    ExpiresByType  text/csv                       "access plus 1 week"
    ExpiresByType  application/vnd.ms-excel       "access plus 1 week"
    ExpiresByType      application/vnd.openxmlformats-officedocument.spreadsheetml.sheet  "access plus 1 week"
    ExpiresByType  image/vnd.djvu                 "access plus 1 week"
    ExpiresByType  application/msword             "access plus 1 week"
    ExpiresByType      application/vnd.openxmlformats-officedocument.wordprocessingml.document  "access plus 1 week"
    ExpiresByType  text/plain                     "access plus 1 week"
    ExpiresByType  application/pdf                "access plus 1 week"
    ExpiresByType  application/postscript         "access plus 1 week"
    ExpiresByType  application/vnd.ms-powerpoint  "access plus 1 week"
    ExpiresByType      application/vnd.openxmlformats-officedocument.presentationml.presentation  "access plus 1 week"
    ExpiresByType  text/rtf                       "access plus 1 week"
    ExpiresByType      application/rtf            "access plus 1 week"

    # кеширование архивов
    ExpiresByType  application/x-gtar             "access plus 1 week"
    ExpiresByType  application/x-rar-compressed   "access plus 1 week"
    ExpiresByType  application/x-tar              "access plus 1 week"
    ExpiresByType  application/gzip               "access plus 1 week"
    ExpiresByType      application/x-gzip         "access plus 1 week"
    ExpiresByType  application/zip                "access plus 1 week"
</IfModule>
