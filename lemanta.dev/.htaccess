    RewriteRule  ^countries/([^/]+(/[^/]+)*)/page[_\-]([^/]+)/?$  index.php?module=Country&url=$1&page=$3  [NC,L,QSA]
    RewriteRule  ^countries/([^/]+(/[^/]+)*)/?$                   index.php?module=Country&url=$1          [NC,L,QSA]
    RewriteRule  ^regions/?$                                      index.php?module=Regions                 [NC,L,QSA]
    RewriteRule  ^regions/page[_\-]([^/]+)/?$                     index.php?module=Regions&page=$1         [NC,L,QSA]
    RewriteRule  ^regions/([^/]+(/[^/]+)*)/page[_\-]([^/]+)/?$    index.php?module=Region&url=$1&page=$3   [NC,L,QSA]
    RewriteRule  ^regions/([^/]+(/[^/]+)*)/?$                     index.php?module=Region&url=$1           [NC,L,QSA]
    RewriteRule  ^towns/?$                                        index.php?module=Towns                   [NC,L,QSA]
    RewriteRule  ^towns/page[_\-]([^/]+)/?$                       index.php?module=Towns&page=$1           [NC,L,QSA]
