    # Новости

    RewriteRule  ^news/?$                                    index.php?module=News                     [NC,L,QSA]
    RewriteRule  ^news/page[_\-]([^/]+)/?$                   index.php?module=News&page=$1             [NC,L,QSA]
    RewriteRule  ^news/([^/]+(/[^/]+)*)/page[_\-]([^/]+)/?$  index.php?module=NewsItem&url=$1&page=$3  [NC,L,QSA]
    RewriteRule  ^news/([^/]+(/[^/]+)*)/?$                   index.php?module=NewsItem&url=$1          [NC,L,QSA]



    # Корзина, отложенные товары

    RewriteRule  ^cart(?:\.class\.php)?/?$                      index.php?module=Cart                                              [NC,L,QSA]
    RewriteRule  ^cart(?:\.class\.php)?/add/([^/]+)/?$          index.php?module=Cart&product_id=$1                                [NC,L,QSA]
    RewriteRule  ^cart(?:\.class\.php)?/delete/([^/]+)/?$       index.php?module=Cart&delete_product_id=$1                         [NC,L,QSA]
    RewriteRule  ^cart(?:\.class\.php)?/quick/?$                index.php?module=Cart&quickcontent=1                               [NC,L,QSA]
    RewriteRule  ^cart(?:\.class\.php)?/quickadd/([^/]+)/?$     index.php?module=Cart&product_id=$1&quickcontent=1                 [NC,L,QSA]
    RewriteRule  ^cart(?:\.class\.php)?/quickdelete/([^/]+)/?$  index.php?module=Cart&delete_product_id=$1&quickcontent=1          [NC,L,QSA]
    RewriteRule  ^defer/?$                                      index.php?module=Cart&defer=1                                      [NC,L,QSA]
    RewriteRule  ^defer/add/([^/]+)/?$                          index.php?module=Cart&defer=1&product_id=$1                        [NC,L,QSA]
    RewriteRule  ^defer/delete/([^/]+)/?$                       index.php?module=Cart&defer=1&delete_product_id=$1                 [NC,L,QSA]
    RewriteRule  ^defer/quick/?$                                index.php?module=Cart&defer=1&quickcontent=1                       [NC,L,QSA]
    RewriteRule  ^defer/quickadd/([^/]+)/?$                     index.php?module=Cart&defer=1&product_id=$1&quickcontent=1         [NC,L,QSA]
    RewriteRule  ^defer/quickdelete/([^/]+)/?$                  index.php?module=Cart&defer=1&delete_product_id=$1&quickcontent=1  [NC,L,QSA]



    # Мгновенный заказ

    RewriteRule  ^fulminant-order/([^/]+)/([^/]+)/?$  index.php?module=FulminantOrder&product_id=$1&variant_id=$2  [NC,L,QSA]



    # Заказы

    RewriteRule  ^order(?:\.class\.php)?/([^/]+)/?$  index.php?module=Order&order_code=$1  [NC,L,QSA]
    RewriteRule  ^order(?:\.class\.php)?/?$          index.php?module=Order                [NC,L,QSA]



    # Аутентификация пользователя и его аккаунт

    RewriteRule  ^logout/?$                                     index.php?module=Logout           [NC,L,QSA]
    RewriteRule  ^login(?:\.class\.php)?/remind/?$              index.php?module=LoginRemind      [NC,L,QSA]
    RewriteRule  ^login(?:\.class\.php)?/?$                     index.php?module=Login            [NC,L,QSA]
    RewriteRule  ^registration(?:\.class\.php)?/?$              index.php?module=Registration     [NC,L,QSA]
    RewriteRule  ^account(?:\.class\.php)?/?$                   index.php?module=Account          [NC,L,QSA]
    RewriteRule  ^account(?:\.class\.php)?/([^/]+(/[^/]+)*)/?$  index.php?module=Account&page=$1  [NC,L,QSA]



    # Информация для внешних сервисов

    RewriteRule  ^robots(_[\.a-z0-9\-]+)?\.txt$   index.php?module=Robots                           [NC,L,QSA]
    RewriteRule  ^sitemap\.xml$                   index.php?module=Sitemap&format=google            [NC,L,QSA]
    RewriteRule  ^pricelist\.xls$                 index.php?module=Pricelist                        [NC,L,QSA]
    RewriteRule  ^yandex(_[\.a-z0-9\-_]*)?\.xml$  index.php?module=Pricelist&format=yandex&nums=$1  [NC,L,QSA]
    RewriteRule  ^hotline(_[0-9_]*)?\.xml$        index.php?module=Hotline&nums=$1                  [NC,L,QSA]
    RewriteRule  ^priceua(_[0-9_]*)?\.xml$        index.php?module=Priceua&nums=$1                  [NC,L,QSA]
    RewriteRule  ^privatmarket(_[0-9_]*)?\.xml$   index.php?module=PrivatMarket&nums=$1             [NC,L,QSA]
    RewriteRule  ^rss/?$                          index.php?module=Rss                              [NC,L,QSA]
    RewriteRule  ^informer/?$                     index.php?module=Rss&mode=informer                [NC,L,QSA]



    # Обратная связь, запрос связи, подключение к уведомлениям

    RewriteRule  ^feedback(?:\.class\.php)?/?$                                     index.php?module=Feedback                                     [NC,L,QSA]
    RewriteRule  ^callme/?$                                                        index.php?module=CallMe                                       [NC,L,QSA]
    RewriteRule  ^notify/?$                                                        index.php?module=NotifyMe                                     [NC,L,QSA]
    RewriteRule  ^notify/(exist|product|article|newsitem|user|all)/?$              index.php?module=NotifyMe&type=$1                             [NC,L,QSA]
    RewriteRule  ^notify/(exist|product|article|newsitem|user)/([^/]+)/?$          index.php?module=NotifyMe&type=$1&object_id=$2                [NC,L,QSA]
    RewriteRule  ^notify/(exist|product|article|newsitem|user)/([^/]+)/([^/]+)/?$  index.php?module=NotifyMe&type=$1&object_id=$2&variant_id=$3  [NC,L,QSA]



    # Скачивание файла

    RewriteRule  ^order(?:\.class\.php)?/([^/]+)/([^/]+)/?$  objects/download.php?order_code=$1&file=$2  [NC,L,QSA]



    # Особый URL. Проверяем его самым последним, чтобы не перекрыть преобразование остальных URL.
    # Так как цепочка условий (RewriteCond) действует только для следующего правила (RewriteRule),
    # повторяем проверку дважды: в первом случае для многостраничного содержимого и во втором -
    # для одностраничного или основной (первой) страницы многостраничного.

    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*?401\.shtml$                                                               [NC]
    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*(admin|design|js|connectors|sounds)(/[^/]+)*/?$                            [NC]
    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*(cart|compare)(/[^/]+)*/?$                                                 [NC]
    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*[^/]*(images|files)(/[^/]+)*/?$                                            [NC]
    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*[^/]+\.(php|js|css|xml|yml|txt|chm|rtf|docx?|xlsx?)$                       [NC]
    RewriteCond  %{REQUEST_URI}                         !^/([^/]+/)*[^/]+\.(gif|jpe?g|png|bmp|swf|zip|rar|avi|mpe?g|mp[0-9]|mkv|wmv|wav|wma)$  [NC]
    RewriteRule  ^([^/]+(/[^/]+)*)/page[_\-]([^/]+)/?$  index.php?special_url=$1&page=$3                                                       [NC,L,QSA]

    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*?401\.shtml$                                                               [NC]
    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*(admin|design|js|connectors|sounds)(/[^/]+)*/?$                            [NC]
    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*(cart|compare)(/[^/]+)*/?$                                                 [NC]
    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*[^/]*(images|files)(/[^/]+)*/?$                                            [NC]
    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*[^/]+\.(php|js|css|xml|yml|txt|chm|rtf|docx?|xlsx?)$                       [NC]
    RewriteCond  %{REQUEST_URI}        !^/([^/]+/)*[^/]+\.(gif|jpe?g|png|bmp|swf|zip|rar|avi|mpe?g|mp[0-9]|mkv|wmv|wav|wma)$  [NC]
    RewriteRule  ^([^/]+(/[^/]+)*)/?$  index.php?special_url=$1                                                               [NC,L,QSA]

# </IfModule>
