# Impera CMS: файл динамичной настройки веб сервера для клиентской части магазина.
#
# ============================================================================
#
# Если PHP запущен как модуль веб сервера Apache, задаем некоторые настройки PHP: ограничение
# на использование памяти, сжатие отдаваемых клиенту страниц, степень сжатия
#
# <IfModule mod_php5.c>

    php_value  memory_limit    64M
    php_value  max_input_vars  12000

    # php_value zlib.output_compression On      #_ZLIB_MARKER
    # php_value zlib.output_compression_level 9 #_ZLIB_MARKER

# </IfModule>



# ----------------------------------------------------------------------------
#
# Данное условие IfModule специально закомментировано, чтобы заключенные в него
# инструкции mod_rewrit-а работали не только на Apache-серверах
#
# <IfModule rewrite_module>

    # Включаем модуль mod_rewrite

    RewriteEngine  on



    # Точка синхронизации 1С и Мой Склад

    RewriteRule  ^cml/?$  admin/index.php?section=CommerceML  [NC,L,QSA]

# </IfModule>
#
# ----------------------------------------------------------------------------



